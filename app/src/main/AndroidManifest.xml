<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skydrop.jenvy">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.skydrop.jenvy.Applications.App"
        android:allowBackup="true"
        android:icon="@mipmap/launchicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launchicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.skydrop.jenvy.Activities.Album_Artist"/>
        <activity android:name="com.skydrop.jenvy.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.skydrop.jenvy.Activities.Playeractivity" />

        <receiver android:name="com.skydrop.jenvy.Notifications.NotificationReceiver" />
    </application>

</manifest>